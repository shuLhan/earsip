	<queryString>
		<![CDATA[SELECT C.nama as metoda_nama, A.tgl,B.nomor,
	B.nama, B.jabatan
FROM t_pemusnahan A
LEFT JOIN r_pemusnahan_metoda C
ON   A.metoda_id = C.id
LEFT JOIN t_pemusnahan_tim B
ON	A.id = B.pemusnahan_id
WHERE pemusnahan_id = $P{pemusnahan_id}::integer
ORDER BY B.nomor ASC]]>
